---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Sirupsen</title>
 <link href="http://appium.io/blog/atom.xml" rel="self"/>
 <link href="http://appium.io/blog"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://blog.appium.io</id>
 <author>
   <name>Appium</name>
   <email>appium-discuss@googlegroups.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://appium.io/blog{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://appium.io/blog{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
</feed>
